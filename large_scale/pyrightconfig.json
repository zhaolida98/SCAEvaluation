{
    "include": [
      "src"
    ],
    
    "exclude": [
      "**/node_modules",
      "**/__pycache__",
      "src/experimental",
      "src/typestubs",
      "reports",
      "projects",
      "cache"
    ],
  
    "ignore": [
      "src/oldstuff"
    ],
  
    "defineConstant": {
      "DEBUG": true
    },
  
    "stubPath": "src/stubs",
    "venv": "env367",
  
    "reportMissingImports": true,
    "reportMissingTypeStubs": true,
  
    "pythonVersion": "3.9",
    "pythonPlatform": "Linux",
  
    "executionEnvironments": [
      {
        "root": "src"
      }
    ]
  }